{
  "manifest_version": 2,

  "name": "PSV3",
  "description": "yes",
  "version": "0.420",
  "icons": {
	"128":"images/psv3icon.png"
  },
  
  "permissions": [
    "https://*/*",
    "http://*/*", 
    "storage",
    "webRequest"
  ],
  
  "browser_action": {
    "default_icon": "images/popicon.png",
    "default_popup": "popup.html"
  },
  
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["psmod.js"]
    }
  ]
}
